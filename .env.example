# Caesar ELO Production Environment
# Copy to .env and fill in values

# Google Maps API (for scraping)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Google OAuth (same as main caesar)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# JWT Secret (generate a random string)
JWT_SECRET=change-me-to-random-string

# URLs (update for your domain)
API_URL=https://caesar-elo.yourdomain.com/api
FRONTEND_URL=https://caesar-elo.yourdomain.com
